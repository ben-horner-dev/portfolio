{
  "name": "benhorner.dev",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@secretlint/secretlint-rule-preset-recommend": "11.2.5",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/coverage-v8": "4.0.14",
    "jsdom": "27.2.0",
    "secretlint": "11.2.5",
    "turbo": "2.6.3",
    "typedoc": "0.28.14",
    "typedoc-github-wiki-theme": "2.1.0",
    "typedoc-plugin-mark-react-functional-components": "0.2.2",
    "typedoc-plugin-markdown": "4.9.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.14"
  },
  "packageManager": "bun@1.2.20",
  "scripts": {
    "semantic-release": "bunx semantic-release",
    "docs": "typedoc --options ./.config/typedoc.json"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/git",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "successCmd": "echo '${nextRelease.gitTag}' > version.txt && echo 'released=true' > release_status.txt"
        }
      ]
    ]
  },
  "trustedDependencies": [],
  "workspaces": [
    "apps/*"
  ],
  "dependencies": {
    "baseline-browser-mapping": "2.9.3",
    "browserslist": "4.28.1",
    "dotenv": "17.2.3",
    "lefthook": "2.0.4"
  },
  "overrides": {
    "jsondiffpatch": "^0.7.2",
    "esbuild": "^0.25.9",
    "mdast-util-to-hast": ">=13.2.1",
    "@conventional-changelog/git-client": ">=2.0.0",
    "glob": ">=10.5.0",
    "js-yaml": ">=4.1.1",
    "playwright": ">=1.57.0",
    "ai": ">=5.0.107",
    "@sentry/node": ">=10.29.0",
    "@sentry/nextjs": ">=10.29.0",
    "@sentry/node-core": ">=10.29.0",
    "vite": ">=7.1.11"
  }
}
