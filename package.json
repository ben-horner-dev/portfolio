{
  "name": "benhorner.dev",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@commitlint/cli": "20.4.1",
    "@commitlint/config-conventional": "20.4.1",
    "@secretlint/secretlint-rule-preset-recommend": "11.3.1",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.2",
    "@vitejs/plugin-react": "5.1.3",
    "@vitest/coverage-v8": "4.0.18",
    "jsdom": "28.0.0",
    "secretlint": "11.3.1",
    "turbo": "2.8.3",
    "typedoc": "0.28.16",
    "typedoc-github-wiki-theme": "2.1.0",
    "typedoc-plugin-mark-react-functional-components": "0.2.2",
    "typedoc-plugin-markdown": "4.9.0",
    "vite-tsconfig-paths": "6.0.5",
    "vitest": "4.0.18"
  },
  "packageManager": "bun@1.2.20",
  "scripts": {
    "semantic-release": "bunx semantic-release",
    "docs": "typedoc --options ./.config/typedoc.json",
    "lint:fix": "turbo lint:fix"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/git",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "successCmd": "echo '${nextRelease.gitTag}' > version.txt && echo 'released=true' > release_status.txt"
        }
      ]
    ]
  },
  "trustedDependencies": [],
  "overrides": {
    "qs": "^6.14.1",
    "@conventional-changelog/git-client": "^2.5.1",
    "lodash": "^4.17.23",
    "lodash-es": "^4.17.23",
    "tar": "^7.5.7",
    "npm": "^11.8.1",
    "@isaacs/brace-expansion": "^5.0.1",
    "path-to-regexp": "^6.3.0",
    "@smithy/config-resolver": "^4.4.6",
    "fast-xml-parser": "^5.3.4",
    "diff": "^8.0.3",
    "esbuild": "^0.27.2",
    "storybook": "^10.2.4"
  },
  "workspaces": [
    "apps/*"
  ],
  "dependencies": {
    "baseline-browser-mapping": "2.9.19",
    "browserslist": "4.28.1",
    "dotenv": "17.2.3",
    "lefthook": "2.1.0"
  }
}
